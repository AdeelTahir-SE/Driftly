{
  "expo": {
    "name": "driftly",
    "slug": "driftly",
    "version": "48.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "config":{
      "GoogleMaps":{"ApiKey": "AIzaSyBlh3CdSRgdqC894ckmxC9f1cJLsTQQcRY"}
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
     
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router",{"origin":"https://driftly.com/"}],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#870101"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
